{
	"name": "dashed-astro",
	"type": "module",
	"version": "3.2.1",
	"scripts": {
		"db:push": "drizzle-kit push:sqlite",
		"db:studio": "drizzle-kit studio",
		"db:generate": "bunx drizzle-kit generate --dialect sqlite --schema ./src/lib/db/schema.ts",
		"dev": "bun --bun astro dev",
		"build": "bun --bun astro build",
		"compile": "bun build ./dist/server/entry.mjs --compile --bytecode --outfile dashed",
		"preview": "bun ./dist/server/entry.mjs",
		"astro": "astro",
		"lint": "biome check && bun run --bun astro check",
		"check": "biome check --write && bun run --bun astro check"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.5",
		"@astrojs/react": "^4.4.2",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@hookform/resolvers": "^5.2.2",
		"@lucide/astro": "^0.554.0",
		"@nurodev/astro-bun": "^2.1.1",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-hover-card": "^1.1.15",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/react-query": "^5.90.10",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"astro": "^5.16.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.7",
		"lucide-react": "^0.554.0",
		"next-themes": "^0.4.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-hook-form": "^7.66.1",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.6",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/bun": "^1.3.3",
		"drizzle-kit": "^0.31.7"
	}
}
